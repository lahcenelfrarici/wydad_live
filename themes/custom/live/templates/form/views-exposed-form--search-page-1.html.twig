{#
/**
 * @file
 * Default theme implementation for a 'form' element.
 *
 * Available variables
 * - attributes: A list of HTML attributes for the wrapper element.
 * - children: The child elements of the form.
 *
 * @see template_preprocess_form()
 *
 * @ingroup themeable
 */
#}


<form {{ attributes }}>
    {{ form.form_build_id }}
    {{ form.form_id }}
    <div class="filter-champs">
        {{ form.search }}
    </div>
    <div class="filter-champs submit-filter">
        {{ form.actions.submit }}
    </div>
    {{ form.form_token }}
</form>

